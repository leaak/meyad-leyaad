@model Tuple<MeyadLeyaad1.Models.Schedule>
@{
    ViewBag.Title = "Scheduler";
}

<h2>Scheduler</h2>
<form role="form" method="post" >
    <fieldset>
     

        @*<div class="form-group">
            <label>יום</label>
            @Html.DropDownListFor(model => model., new SelectList(new[]{"ארון",
                                    "ספה", "כסא","שולחן" , "מיטות", "ספריה","אחר"}))
        </div>*@
        <!-- Change this to a button or input when using this as a form -->
        <button type="submit" class="btn btn-primary btn-block">צור לוז לנהג</button>
       
    </fieldset>

    <div>
        <table class="table-gray">
            <tr>
                <th>המוצר</th>
                <th>שם התורם</th>
                <th>עיר</th>
                <th>כתובת</th>
                <th>טלפון ניד</th>
                <th>בין השעות</th>
                
            </tr>

            @foreach (var item in ViewBag.dict)
            {
                <tr>
                    <td>@item["המוצר"]</td>
                    <td>@item["שם התורם"]</td>
                    <td>@item["עיר"]</td>
                    <td>@item["כתובת"]</td>
                    <td>@item["טלפון נייד"]</td>
                    <td>@item["בין השעות"]</td>
                    

                </tr>
            }
        </table>
</div>

</form>
